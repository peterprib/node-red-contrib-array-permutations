{
  "name": "node-red-contrib-array-permutations",
  "version": "0.0.5",
  "description": "Node-red permutations of an array as repeat or messages.",
  "dependencies": {
    "node-red-contrib-array-permutations": "file:",
    "node-red-contrib-logger": "*"
  },
  "devDependencies": {
    "mocha": "*",
    "node-red": "*",
    "node-red-node-test-helper": "*"
  },
  "scripts": {
    "test": "mocha \"test/**/t*.js\"",
    "startNodeRed": "node-red --userDir ./.temp --title '***test***' --setting ./test/data/settings.js ./test/data/flow.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peterprib/node-red-contrib-array-permutations.git"
  },
  "keywords": [
    "node-red",
    "loop",
    "permutations",
    "heap",
    "random",
    "combinations",
    "circular",
    "repeat",
    "array"
  ],
  "node-red": {
    "nodes": {
      "array-permutations": "array-permutations/array-permutations.js"
    }
  },
  "author": "Peter Prib",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/peterprib/node-red-contrib-array-permutations/issues"
  },
  "homepage": "https://github.com/peterprib/node-red-contrib-array-permutations#readme"
}
